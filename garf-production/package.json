{
  "name": "garf-grouping-engine",
  "version": "1.0.0",
  "description": "GARF Grouping Engine with Supabase Integration",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev:api": "tsx watch server.ts",
    "dev:web": "npm --prefix frontend run dev",
    "dev": "concurrently -n API,WEB -c blue,green \"npm:dev:api\" \"npm:dev:web\"",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "migrate": "psql $DATABASE_URL < schema.sql",
    "seed": "tsx scripts/seed.ts",
    "specify": "powershell -ExecutionPolicy Bypass -File ./specify.ps1",
    "plan": "powershell -ExecutionPolicy Bypass -File ./plan.ps1",
    "tasks": "powershell -ExecutionPolicy Bypass -File ./tasks.ps1"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.2",
    "@supabase/supabase-js": "^2.39.0",
    "fastify": "^4.25.1",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "grouping",
    "algorithm",
    "supabase",
    "lovable",
    "fastify",
    "typescript"
  ],
  "author": "GARF Team",
  "license": "MIT"
}
